{
    "family": "next-uk-post-codes",
    "executionRoleArn": "arn:aws:iam::924308068754:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "next-uk-post-codes",
            "image": "924308068754.dkr.ecr.us-east-2.amazonaws.com/newecsrepo:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
